generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model data_formulario {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  Ingresos   String
  Yummy      Boolean
  Extranjero Boolean
  Cedula     String
  PrimerNombre   String
  SegundoNombre  String
  PrimerApellido String
  SegundoApellido String
  Dirrecion  String
  Pais       String
  name       String
  email      String
  subject    String
  message    String
}

model clerk {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  id_clerk String
  number String
  gmail String
  img String
  username String
  first_name String
  last_name String
}
