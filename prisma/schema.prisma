generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id         String      @id @default(auto()) @map("_id") @db.ObjectId
  Ingresos   String
  Yummy      Boolean
  Extranjero Boolean
  Cedula     String
  PrimerNombre   String
  SegundoNombre  String
  PrimerApellido String
  SegundoApellido String
  Dirrecion  String
  Pais       String
  name       String
  email      String
  subject    String
  message    String
}
