generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model data_formulario {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  id_clerk String @unique
  Ingresos   String
  Yummy      Boolean
  Extranjero Boolean
  Cedula     String
  PrimerNombre   String
  SegundoNombre  String
  PrimerApellido String
  SegundoApellido String
  Dirrecion  String
  Pais       String
}

model clerk {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  id_clerk String @unique
  number String
  gmail String
  img String
  username String
  first_name String
  last_name String
  estado_formulario String
  fecha String
  estado_proceso String
  aprobacion_yummy Boolean
  aprobacion_tvs Boolean
  aprobacion_final Boolean
}

model documentos {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  id_clerk String @unique
  cedula String
  dashoard_yummy String
  rif String
  persona_cedula String
}

model admin {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  id_clerk String @unique
  autorizacion Boolean
}

model admin_yummy {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  id_clerk String @unique
  autorizacion Boolean
}

model correo_iniciando_estimacion {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  id_clerk String @unique
  gmail String @unique
  iniciando Boolean
  fecha String
}