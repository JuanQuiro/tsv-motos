generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id         String      @id @default(auto()) @map("_id") @db.ObjectId
  Ingresos   String
  Yummy      Boolean
  Extranjero Boolean
  Cedula     String
  PrimerNombre   String
  SegundoNombre  String
  PrimerApellido String
  SegundoApellido String
  Dirrecion  String
  Pais       String
  name       String
  email      String
  subject    String
  message    String
}

model Auth {
  id    String      @id @default(auto()) @map("_id") @db.ObjectId
  email String  @unique
  lastname  String
  name String
  numberPhone Int
}

model Elegibilidad {
  id    String      @id @default(auto()) @map("_id") @db.ObjectId
  ingresosPromedio String
  archivoElegilidad String
}

model Identidad {
  id    String      @id @default(auto()) @map("_id") @db.ObjectId
  cedula Int
  archivoCedula String
  archivoHolding String
}