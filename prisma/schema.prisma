generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  fecha DateTime
}

model Auth {
  id    Int     @id @default(autoincrement())
  email String  @unique
  lastname  String
  name String
}