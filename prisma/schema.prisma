generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String
  fecha DateTime
}

model Auth {
  id    Int     @id @default(autoincrement())
  email String  @unique
  lastname  String
  name String
  numberPhone Int
}

model Elegibilidad {
  id    Int     @id @default(autoincrement())
  ingresosPromedio String
  archivoElegilidad String
}

model Identidad {
  id    Int     @id @default(autoincrement())
  cedula Int
  archivoCedula String
  archivoHolding String
}