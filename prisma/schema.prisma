generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model data_formulario {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  Ingresos   String
  Yummy      Boolean
  Extranjero Boolean
  Cedula     String
  PrimerNombre   String
  SegundoNombre  String
  PrimerApellido String
  SegundoApellido String
  Dirrecion  String
  Pais       String
  name       String
  email      String
  subject    String
  message    String
}

model clerck {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  object String
  primary_email_address_id String
  primary_phone_number_id String
  primary_web3_wallet_id String
  username String
  first_name String
  last_name String
  profile_image_url String
  image_url String
}
